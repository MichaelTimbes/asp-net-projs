@model IEnumerable<Demo.Models.UserModel>
<!-- Index Page to see what users are on the current database but without functionality to
    delete or edit users-->
@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>
<p> List of Users:</p>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                   <!-- @Html.DisplayNameFor(model => model.User_Name)-->
                User Name:
                </th>
                <!--<th>
                    @Html.DisplayNameFor(model => model.User_Password)
                </th>-->
                <th>
                    <!--@Html.DisplayNameFor(model => model.User_SignIn_Status)-->
                User Signed In:
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.User_Name)
            </td>
           <!-- <td>
                @Html.DisplayFor(modelItem => item.User_Password)
            </td>-->
            <td>
                @Html.DisplayFor(modelItem => item.User_SignIn_Status)
            </td>
           <!-- <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>-->
        </tr>
}
    </tbody>
</table>
