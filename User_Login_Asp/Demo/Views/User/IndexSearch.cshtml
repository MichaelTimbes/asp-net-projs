@model IEnumerable<Demo.Models.UserModel>

@{
    ViewData["Title"] = "AdminIndex";
    Layout = "_Layoutv2";
}

<h2>Index</h2>
<p> Search of Users:</p>
<table class="table">
    <thead>
        <tr>
                <th>
                   <!-- @Html.DisplayNameFor(model => model.User_Name)-->
                User Name:
                </th>
                <!--<th>
                    @Html.DisplayNameFor(model => model.User_Password)
                </th>-->
                <th>
                    <!--@Html.DisplayNameFor(model => model.User_SignIn_Status)-->
                User Signed In:
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.User_Name)
            </td>
           <!-- <td>
                @Html.DisplayFor(modelItem => item.User_Password)
            </td>-->
            <td>
                @Html.DisplayFor(modelItem => item.User_SignIn_Status)
            </td>
            <td>
                <a asp-area="" asp-controller="UserProfile" asp-action="LinkToProfileFromLayout"  asp-route-id="@item.UserProfileID">Profile</a>
            </td>
        </tr>
}
    </tbody>
</table>
